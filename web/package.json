{
  "name": "web",
  "version": "0.2.0",
  "scripts": {
    "start": "ng serve --host 0.0.0.0",
    "lint": "ng lint",
    "test": "ng test",
    "build": "ng build",
    "build:prod": "ng build --configuration=\"production\" && npm run gzipper",
    "build:dev": "ng build --configuration=\"development\"",
    "disable:cache": "ng cache disable && ng cache off",
    "gzipper": "gzipper compress --gzip-level=6 ./dist/web",
    "update": "npm-check --update --save-exact"
  },
